{
  "manifest_version": 2,
  "name": "Bookmark Saver",
  "version": "0.1",
  "description": "Example Chrome Redux extension",
  "browser_action": {
    "default_icon": {
      "16": "assets/agorafire-tv-16-16.png",
      "32": "assets/agorafire-tv-32-32.png",
      "48": "assets/agorafire-tv-48-48.png",
      "128": "assets/agorafire-tv-128-128.png"
    },
    "default_title": "Click to save bookmark",
    "default_popup": "pages/popup.html"
  },
  "icons": {
    "16": "assets/agorafire-tv-16-16.png",
    "32": "assets/agorafire-tv-32-32.png",
    "48": "assets/agorafire-tv-48-48.png",
    "128": "assets/agorafire-tv-128-128.png"
  },
  "author": "Bruno Antunes <bruno@thoughtbot.com>",
  "background": {
    "page": "pages/background.html"
  },
  "permissions": [ "tabs", "notifications" ],
  "content_security_policy":"script-src 'self' https://ssl.gstatic.com/* https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; object-src 'self'",
  "minimum_chrome_version": "60"
}
